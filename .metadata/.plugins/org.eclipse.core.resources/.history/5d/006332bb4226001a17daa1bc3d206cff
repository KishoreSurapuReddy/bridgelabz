package com.bridgelabz.logicalprograms;

import java.util.Random;

public class Util {
	
	public static void calculate(int min,int max) {
		int wins=0;
		int i;
		int losses = 0;
		int stack = 10;
		int goal = 16;
		Random rand = new Random();
		for( i=1;i<=20;i++) {
			int res = rand.nextInt(max-min);
			//System.out.println(res);
			if(res==0) {
				System.out.println("player has loss everything!..");
			}else if(res>=16) {
				wins++;
			}else if(res<16) {
				losses++;
			}
		}
		double win = wins/(double)i*100;
		double loss = losses/(double)i*100;
		System.out.println("win % is: "+win);
		System.out.println("loss % is: "+loss);
		return;
	}


}
